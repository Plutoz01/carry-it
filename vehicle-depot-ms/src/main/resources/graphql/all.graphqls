type Query {
    getAllDepot(count: Int, offset: Int): [Depot]!
    getAllVehicle: [Vehicle]!
    getVehicleById(id: ID): Vehicle
}

type Depot {
    id: ID!
    name: String!
    vehicles: [Vehicle]!
    vehicleCount: Int!
}

type Vehicle {
    id: ID!
    licencePlate: String!
    depotId: ID!
    depot: Depot!
}
